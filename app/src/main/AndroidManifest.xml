<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blogspot.ramannada.movieapp">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="AA_MODELS"
            android:value="com.blogspot.ramannada.movieapp.user com.blogspot.ramannada.movieapp.movie" />
        <meta-data android:name="AA_DB_NAME" android:value="prepop.db" />
        <meta-data android:name="AA_DB_VERSION" android:value="1" />
        <meta-data android:name="AA_SERIALIZERS"
            android:value="my.package.CustomTypeSerializer,my.package.AnotherCustomeTypeSerializer" />

        <activity android:name=".MainActivity"
            android:theme="@style/SplashScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>